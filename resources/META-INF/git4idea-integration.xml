<idea-plugin>
    <actions>
        <action id="GitRebaseOnGreenMasterCommitAction"
                class="org.jetbrains.kotlin.test.helper.actions.git.RebaseOnGreenMasterCommitAction"
                icon="AllIcons.Vcs.CommitNode">
            <add-to-group group-id="Git.Branch" anchor="after" relative-to-action="GitRebaseBranchAction"/>
        </action>

        <action id="GitResetMasterToGreenCommitAction"
                class="org.jetbrains.kotlin.test.helper.actions.git.ResetMasterToGreenCommitAction"
                icon="AllIcons.Vcs.CommitNode">
            <add-to-group group-id="Git.Branch" anchor="after" relative-to-action="GitRebaseOnGreenMasterCommitAction"/>
        </action>
    </actions>
</idea-plugin>
